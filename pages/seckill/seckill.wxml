                    <image class="handpick-image" src="../images/handpick/图层 1.png"></image>
                </view>
                <view class="handpick-text handpick-margin">
                    <text class="handpick-title handpick-title-two">每日特卖</text>
                    <image class="handpick-image" src="../images/handpick/图层 2.png"></image>
                </view>
                <view class="handpick-text">
                    <text class="handpick-title handpick-title-three">榜单名称</text>
                    <image class="handpick-image" src="../images/handpick/图层 3.png"></image>
                </view>
            </view>
        </van-tab>
        <van-tab title="拼团" name="b">
