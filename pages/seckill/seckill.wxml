                <van-tabs active="{{ active }}" bind:click="requestcontent">
                    <van-tab wx:for="{{arr}}" wx:key='index' title="{{item}}">
                        <view class="seckill-banner" wx:for="{{name}}" wx:key="index" bindtap="particulars" data-id='{{item.id}}'>
                            <view class="seckill-commodity">
                                <view class="commodity-tu">
                                    <image src="../images/handpick/shangping.png"></image>
                                </view>
                                <view class="commodity-text">
                                    <view class="commodity-title">
                                        {{item.hfGoods.hfName}}
                                    </view>
                                    <view class="commodity-money">
                                        <view class="commodity-number">
                                            <view class="commodity-j">已抢3600件</view>
                                            <view class="commodity-shu">
                                                <view class="commodity-dollar">
                                                    ￥{{item.price}}
                                                </view>
                                                <view class="commodity-del">
                                                    ￥{{item.isDeleted}}
                                                </view>
                                            </view>
                                        </view>
                                        <view class="commodity-qg">立即抢购</view>
                                    </view>
                                </view>
                            </view>
                        </view>
                    </van-tab>
                </van-tabs>
            </view>
        </van-tab>
