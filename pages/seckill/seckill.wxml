<view class="seckill">
    <view class="seckill-header">
        <view class="header-title">
            <view class="header-t">
                <view class="header-tu"></view>
                <text class="header-text">思创科技</text>
            </view>
            <view class="header-fl">
                <view class="header-sao header-margin">
                    <image src="../images/tabs/扫一扫.png" class="sao-tu" />
                    <view class="sao">扫一扫</view>
                </view>
                <view class="header-sao">
                    <image src="../images/tabs/xiaoxis.png" class="sao-tu" />
                    <view class="sao">客服</view>
                </view>
            </view>
        </view>
        <view class="sou">
            <image src="../images/tabs/sousuo.png" class="sao-sou" />
            <input type="text" class="input-sou" />
        </view>
    </view>
    <view class="seckill-img">
        <swiper vertical="{{false}}" indicator-dots="{{true}}" autoplay="{{true}}">
            <swiper-item>
                <image src="../images/tabs/shoutu.png" class="img-size"></image>
            </swiper-item>
            <swiper-item>
                <image src="../images/tabs/shoutu.png" class="img-size"></image>
            </swiper-item>
