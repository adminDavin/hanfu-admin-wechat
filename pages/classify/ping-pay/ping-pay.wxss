.ping-pay {
    width: 750rpx;
    height: 100%;
}

/* 头部 */
.pay-header {
    width: 100%;
    height: 94rpx;
    background-color: #FFC703;
}

.table-number {
    width: 44rpx;
    height: 44rpx;
    background: rgba(255, 255, 255, 1);
    border-radius: 5rpx;
    padding: 7rpx;
}

.header-table {
    float: right;
    margin-top: 27rpx;
    margin-right: 36rpx;
}

.table-time {
    font-size: 32rpx;
    font-family: PingFang SC;
    font-weight: 500;
    color: rgba(21, 21, 21, 1);
    margin: 0 8rpx 0 6rpx;
}

.notice {
    width: 141rpx;
    height: 37rpx;
    background: rgba(255, 255, 255, 1);
    opacity: 0.67;
    font-size: 24rpx;
    line-height: 37rpx;
    text-align: center;
    font-family: PingFang SC;
    font-weight: 500;
    color: rgba(255, 126, 0, 1);
    border-radius: 19rpx 0 0 19rpx;
    float: right;
    margin-top: 29rpx;
}

/* banner */
.pay-shpping {
    width: 100%;
    height: 240rpx;
    display: flex;
}

.pay-image {
    width: 200rpx;
    height: 200rpx;
    margin: 20rpx 21rpx 20rpx 19rpx;
}

.pay-content {
    width: 487rpx;
    margin-top: 27rpx;
}

.content-title {
    font-size: 26rpx;
    font-family: PingFang SC;
    font-weight: 500;
    color: rgba(21, 21, 21, 1);
}

.title-box {
    margin-top: 42rpx;
}

.title-number {
    font-size: 24rpx;
    font-family: PingFang SC;
    font-weight: 400;
    color: rgba(153, 153, 153, 1);
}

.title-money {
    display: flex;
    align-items: center;
}

.money-number {
    font-size: 32rpx;
    font-family: PingFang SC;
    font-weight: bold;
    color: rgba(255, 0, 0, 1);
}

.money-del {
    font-size: 24rpx;
    font-family: PingFang SC;
    font-weight: 400;
    text-decoration: line-through;
    color: rgba(153, 153, 153, 1);
    margin-left: 9rpx;
}

/* 拼团成功 */
.par-succeed {
    width: 750rpx;
    margin-top: 20rpx;
    overflow: hidden;
}

.succeed {
    height: 29rpx;
    margin-top: 20rpx;
    text-align: center;
}

.succeed-text {
    font-size: 30rpx;
    font-family: PingFang SC;
    font-weight: 500;
    color: rgba(21, 21, 21, 1);
}

.succeed-color {
    color: #FF4141;
}

.succeed-photos {
    margin-left: 49rpx;
    margin-right: 52rpx;
    display: flex;
    justify-content: space-around;
    margin-top: 20rpx;
}

.photos-head {
    width: 90rpx;
    display: flex;
    flex-direction: column;
    align-items: center;
    position: relative;
    margin-top: 29rpx;
}

.photo-tubbish {
    width: 60rpx;
    height: 25rpx;
    line-height: 25rpx;
    text-align: center;
    font-size: 20rpx;
    font-family: PingFang SC;
    font-weight: 500;
    background-color: #FF3333;
    color: rgba(255, 255, 255, 1);
    position: absolute;
    top: -5rpx;
    left: 15rpx;
    border-radius: 10rpx;
}

.photo-heads {
    width: 90rpx;
    height: 90rpx;
    border-radius: 50%;
    background-size: 90rpx 90rpx;
    background-image: url('data:image/png;base64,');
}

.heads-wen {
    line-height: 90rpx;
    font-size: 50rpx;
    color: #FF3333;
    font-weight: 500;
    text-align: center;

}

.photo-title {
    width: 90rpx;
    font-size: 24rpx;
    font-family: PingFang SC;
    font-weight: 400;
    color: rgba(21, 21, 21, 1);
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    margin-top: 10rpx;
}

.more-all {
    display: flex;
    align-items: center;
    width: 124rpx;
    height: 23rpx;
    line-height: 23rpx;
    margin: 0 auto;
    margin-top: 39rpx;
}

.all-text {
    font-size: 24rpx;
    font-family: PingFang SC;
    font-weight: 400;
    color: rgba(153, 153, 153, 1);
}

.all-image {
    width: 18rpx;
    height: 10rpx;
    margin-left: 10rpx;
}

/* 邀请好友 */
.invite {
    width: 560rpx;
    height: 80rpx;
    background: rgba(255, 51, 51, 1);
    border-radius: 40rpx;
    line-height: 80rpx;
    text-align: center;
    font-size: 34rpx;
    font-family: PingFang SC;
    font-weight: 500;
    color: rgba(255, 255, 255, 1);
    margin: 0 auto;
    margin-top: 52rpx;
}

/* 拼团失败 */
.table-failure {
    display: block;
    width: 137rpx;
    font-size: 34rpx;
    font-family: PingFang SC;
    font-weight: bold;
    color: rgba(21, 21, 21, 1);
    line-height: 94rpx;
    margin: 0 auto;
}
/* 拼团成功 */
.table-color {
    color: #FF3333;
}