<view class="puzzle-particulars">
    <!-- 图片 -->
    <view class="header-images">
        <block>
            <swiper vertical="{{false}}" indicator-dots="{{flase}}" autoplay="{{flase}}">
                <swiper-item>
                    <image src="" class="images-head" />
                </swiper-item>
                <swiper-item>
                    <image src="" class="images-head" />
                </swiper-item>
                <swiper-item>
                    <image src="" class="images-head" />
                </swiper-item>
                <swiper-item>
                    <image src="" class="images-head" />
                </swiper-item>
                <swiper-item>
                    <image src="" class="images-head" />
                </swiper-item>
            </swiper>
        </block>
        <view class="images-return">
            <image src="../../images/tabs/jinru (4).png" class="return" />
        </view>
        <view class="images-index">1/5</view>
    </view>
    <!-- 商品价格及名称 -->
    <view class="productName">
        <view class="productName-box">
            <view class="productName-dollor">
                <text class="dollor">¥</text>
                <text class="money">{{dongtai.price}}</text>
                <text class="money-del">¥{{dongtai.isDeleted}}</text>
                <text class="news">新品</text>
            </view>
            <view class="productName-collect">
                <!-- <view class="collect-one">
                    <image src="../../images/tabs/jiangjia.png" class="depreciate-image" />
                    <text class="depreciate-text">降价通知</text>
                </view> -->
                <view class="collect-one collect-margin" bindtap="attention">
                    <image src="../../images/tabs/chakantieziguanzhu.png" class="depreciate-image" />
                    <text class="depreciate-text">12.5万</text>
                </view>
            </view>
        </view>
        <!-- 拼团 -->
        <view class="puzzle">
            <view class="puzzle-times">
                <image src="../../images/tabs/naozhong.png" class="times-begin" />
                <text class="over">距开始</text>
                <text class="over-number">00</text>
                <text class="over">时</text>
                <text class="over-number">00</text>
                <text class="over">分</text>
                <text class="over-number">00</text>
                <text class="over">秒</text>
                <view></view>
            </view>
            <view class="puzzle-mark">已拼成0件</view>
        </view>
        <view class="puzzle" style="display:none">
            <view class="puzzle-times">
                <image src="../../images/tabs/naozhong.png" class="times-begin" />
                <text class="over">距结束</text>
                <text class="over-number">00</text>
                <text class="over">时</text>
                <text class="over-number">00</text>
                <text class="over">分</text>
                <text class="over-number">00</text>
                <text class="over">秒</text>
            </view>
            <view class="puzzle-mark">已拼成0件</view>
        </view>
        <view class="productName-box-two">
            <view class="productName-table">
                {{dongtai.hfGoods.hfName}}
            </view>
            <view class="table-box" bindtap="fenxiang">
                <image src="../../images/tabs/fenxiang.png" class="table-images" />
                <text class="share">分享</text>
            </view>
        </view>
        <!-- <view class="partner">
            <view class="partner-box">
                <text class="partner-title">参团小伙伴</text>
                <view class="partner-image">
                    <image src="" class="partner-images" />
                    <image src="" class="partner-images partner-p" />
                    <image src="" class="partner-images partner-p1" />
                    <image src="" class="partner-images partner-p2" />
                    <image src="" class="partner-images partner-p3" />
                    <image src="" class="partner-images partner-p4" />
                </view>
            </view>
            <image src="../../images/tabs/diandian.png" class="selected-images" />
        </view> -->
    </view>
    <!-- 优惠项 -->
    <view class="discounts">
        <!-- <text class="discounts-title">优惠</text>
        <text class="discounts-table table-left">每满200减30</text> -->
        <text class="discounts-table table-margin">包邮</text>
    </view>
    <!-- 地址及已选 -->
    <view class="site">
        <view class="site-selected">
            <view class="selected-box">
                <text class="selected">已选</text>
                <text class="selected-text" wx:for="{{dongtai.hfGoodsSpec}}" wx:key="index">{{item.hfValue}}</text>
            </view>
            <image src="../../images/tabs/diandian.png" class="selected-images" />
        </view>
        <view class="site-selected">
            <view class="selected-box delivery-box">
                <text class="selected">送至</text>
                <image src="../../images/tabs/weizhi.png" class="delivery-image" />
                <text class="selected-text">{{sites}}</text>
            </view>
            <image src="../../images/tabs/diandian.png" class="selected-images" />
        </view>
        <view class="site-freight">
            <text class="selected">运费</text>
            <text class="selected-text">免运费</text>
        </view>
        <view class="site-refund" bindtap="showModal">
            <view class="refund-flex">
                <image src="../../images/tabs/fangxingou.png" class="refund-image" />
                <view class="refund-box">
                    <text class="refund-text">闪电退款</text>
                    <view class="refund-d"></view>
                    <text class="refund-text">极速审核</text>
                    <view class="refund-d"></view>
                    <text class="refund-text">7天价保</text>
                </view>
            </view>
            <image src="../../images/tabs/diandian.png" class="selected-images"/>
        </view>
        <view class="site-safeguard" bindtap="showModal">
            <image src="../../images/tabs/对.png" class="safeguard-image" />
            <text class="safeguard-text">店铺发货&售后</text>
            <image src="../../images/tabs/对.png" class="safeguard-image" />
            <text class="safeguard-text">7天无理由退货</text>
            <image src="../../images/tabs/对.png" class="safeguard-image" />
            <text class="safeguard-text">货到付款</text>
        </view>
    </view>
