        </view>
    </view>
    <!-- 商品详情 -->
    <view class="description">
        <view class="description-title">商品详情</view>
        <view class="description-contents">
            {{dongtai.hfGoods.goodsDesc}}
        </view>
    </view>
    <!-- 底部 -->
    <view class="footer">
        <view class="footter-service">
            <image src="../../images/tabs/xiaoxi.png" class="footer-image" />
            <text class="foot-text">客服</text>
        </view>
        <view class="footer-shppingcar" bindtap="dandushpping">单独购买</view>
        <view class="footer-shppingcar purchase" bindtap="yijianpintuan">一键拼团</view>
    </view>
    <!-- 服务弹窗 -->
    <view class="modals modals-bottom-dialog" hidden="{{hideModal}}">
        <view class="modals-cancel" bindtap="hideModal"></view>
        <view class="bottom-dialog-body bottom-pos" animation="{{animationData}}">
            <view class="services">
                <text class="services-title">基础服务</text>
                <scroll-view scroll-y="true" class="services services-bottom">
                    <view class="services-content services-margin">
                        <image src="../../images/tabs/对勾.png" class="kind-image" />
                        <view class="services-kind">
                            <text class="kind-table">7天无理由</text>
                            <text class="kind-text">
                                满足7天无理由退换货的前提下，包邮商品需要买家承担退货邮费，非包邮需要买家承担发货和退货邮费
                            </text>
                        </view>
                    </view>
                    <view class="services-content services-margin">
                        <image src="../../images/tabs/对勾.png" class="kind-image" />
                        <view class="services-kind">
                            <text class="kind-table">15天退货</text>
                            <text class="kind-text">15天退货，退货邮费卖家承担</text>
                        </view>
                    </view>
                </scroll-view>
                <view class="complete" bindtap="hideModal">完成</view>
            </view>
        </view>
    </view>
    <!-- 查看更多拼团 -->
    <view class="modalDlg" wx:if="{{showModal}}">
        <image src="../../images/tabs/guanbi.png" class="modalDlg-over" bindtap="close_mask" />
        <text class="modalDlg-text">正在开团</text>
        <view class="modalDlg-box">
            <view class="open-client" wx:for="{{number}}" wx:key="index">
                <view class="open-client-box">
                    <image src="../../images/tabs/t (1).png" class="client-head" />
                    <view class="client-flex">
                        <text class="client-title title-margin">{{item.name}}</text>
                        <text class="client-second">2019.12.09 19:00</text>
                    </view>
                </view>
                <view class="client-roll">
                    <view class="client-time time-heights">
                        <view class="photos">
                            <text class="client-photo">还差</text>
                            <text class="client-photo photo-color">{{item.number}}人</text>
                            <text class="client-photo">拼成</text>
                        </view>
                    </view>
                    <view class="client-button">去拼单</view>
                </view>
            </view>
        </view>
        <text class="open-text">仅显示10个正在开团的人</text>
    </view>
    <view class="mask" wx:if="{{showModal}}"></view>
</view>