.editaddress {
  width: 100%;
  height: 100%;
  background-color: #f8f8f8;
}

.del {
  width: 56rpx;
  font-size: 28rpx;
  font-family: PingFang SC;
  font-weight: 400;
  color: rgba(51, 51, 51, 1);
  float: right;
  margin-right: 24rpx;
  background-color: white;
}

/* content */

.addr-content {
  width: 100%;
  padding: 30rpx 24rpx 0;
  box-sizing: border-box;
  background-color: white;
}

.addr-card {
  height: 98rpx;
  border-bottom: 1rpx solid rgba(229, 229, 229, 1);
  display: flex;
  align-items: center;
}

.text-card {
  display: flex;
}

.card-left-item {
  width: 120rpx;
  line-height: 98rpx;
  font-size: 30rpx;
  font-family: PingFang SC;
  font-weight: 400;
  color: rgba(102, 102, 102, 1);
  margin-right: 28rpx;
}

.dingwei {
  width: 100%;
  display: flex;
  justify-content: flex-end;
  align-items: center;
}

/* setmoren */

.setmoren {
  margin-top: 3rpx;
  width: 100%;
  height: 130rx;
  padding: 30rpx 20rpx;
  box-sizing: border-box;
  display: flex;
  justify-content: space-between;
  align-items: center;
  background-color: white;
}

.setmoren-left {
  width: 420rpx;
  height: 100%;
}

.set-ziti {
  font-size: 30rpx;
  line-height: 42rpx;
  font-weight: 400;
  color: rgba(0, 0, 0, 1);
}

/* footer */

.footer {
  position: fixed;
  bottom: 20rpx;
  width: 709rpx;
  height: 80rpx;
  background: rgba(255, 0, 0, 1);
  margin: 0 23rpx;
  border-radius: 40rpx;
  line-height: 80rpx;
  font-size: 32rpx;
  font-family: PingFang SC;
  color: rgba(255, 255, 255, 1);
  text-align: center;
}
