/* pages/address/address.wxss */
.address{
  width: 100%;
  height:100%;
}
.address-card{
  width:100%;
  height:127rpx;
  padding:0 24rpx;
  box-sizing: border-box;
  border-bottom: 1rpx solid rgba(229,229,229,1);
}
.address-card-top{
  width:100%;
  display: flex;
  align-items: center;
font-size:34rpx;
line-height: 60rpx;
font-weight:500;
color:rgba(21,21,21,1);
}
.moren {
  width: 70rpx;
  height: 35rpx;
  background: rgba(255, 0, 0, 1);
  border-radius: 10rpx;
  text-align: center;
  line-height: 35rpx;
  font-size: 24rpx;
  font-family: PingFang SC;
  font-weight: 500;
  color: rgba(255, 255, 255, 1);
  margin-right: 32rpx;
}
.address-card-bottom{
  width:100%;
    display: flex;
  align-items: center;
  justify-content: space-between;
}
.top-ziti{
font-size:34rpx;
font-weight:500;
color:rgba(21,21,21,1);
}
.bot-ziti{
  width:600px;
  line-height: 60rpx;
font-size:24rpx;
font-weight:400;
color:rgba(51,51,51,1);
overflow: hidden;
text-overflow: ellipsis;
white-space: nowrap;
}
/* footer */
.footer{
  position: fixed;
  bottom:20rpx;
  width:709rpx;
height:80rpx;
background:rgba(255,0,0,1);
margin:0 23rpx;
border-radius:40rpx;
line-height: 80rpx;
font-size:32rpx;
font-family:PingFang SC;
color:rgba(255,255,255,1);
text-align: center;
}