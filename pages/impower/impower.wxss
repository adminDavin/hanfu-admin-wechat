.mask {
  width: 750rpx;
  height: 1206rpx;
  background-color: #ADADAD;
  overflow: hidden;
}
.mask-dl {
  display: flex;
  flex-direction:column;
  width: 558rpx;
  height: 550rpx;
  margin: 0 auto;
  margin-top: 166rpx;
  border-radius: 35rpx;
  background-color: #FFFFFF;
}
.mask-tible {
  width: 500rpx;
  text-align: center;
  line-height: 102rpx;
  margin: 0 auto;
  font-size: 34rpx;
  font-weight: bold;
  color: black;
  border-bottom: 1px solid #E5E5E5;
}
.mask-tibles {
  font-size: 26rpx;
  margin: 0 auto;
  margin-top: 30rpx;
  margin-bottom: 42rpx;
  color: #666666;
}
.mask-head {
  width: 200rpx;
  height: 200rpx;
  border-radius: 50%;
  background: #E4E4E4;
  margin: 0 auto;
}
.header{
  line-height: 200rpx;
  text-align: center;
}
.mask-buttom {
  margin: 0 auto;
  margin-top: 42rpx;
  height: 62rpx;
  font-size: 28rpx;
  width: 420rpx;
  /* background: #000; */
}
.buttom {
  float: left;
  width: 183rpx;
  height: 62rpx;
  line-height: 62rpx;
  text-align: center;
  color: #878787;
  border-radius: 10rpx;
  border: 1px solid #878787;
}
.buttom-two {
  float: right;
  width: 183rpx;
