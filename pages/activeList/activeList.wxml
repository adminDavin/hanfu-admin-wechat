<view style="padding:40rpx 30rpx; ">
  <view wx:for="{{activeList}}" wx:for-index="idx" wx:for-item="item"  style="display:flex;justify-content:space-between;background:#fff;align-items:center;margin-bottom:30rpx;padding:10rpx;">
    <view style="margin-left:40rpx;">
      {{item.activityName }}
    </view>
    <view style="display:flex;align-items:center;padding:40rpx;" wx:if="{{item.isTimingStart==1}}" data-id="{{item.id}}"  data-type="{{item.type}}" bindtap="goVote">
      <view style="color:#DC0000;margin-right:15rpx;" wx:if="{{item.type=='ticket_count'}}">去投票</view>
      <view style="color:#DC0000;margin-right:15rpx;" wx:if="{{item.type=='score'}}">去打分</view>
      <image src="../img/back.png" style="width:20rpx;height:24rpx;"></image>
    </view>
    <view style="display:flex;align-items:center;padding:40rpx;" wx:if="{{item.isTimingStart==0}}">
      <image src="../img/wei.png" style="width:36rpx;height:36rpx;"></image>
      <view style="color:#DC0000;margin-right:15rpx;color:#999999;margin-left:10rpx">活动尚未开启</view>
    </view>
  </view>
</view>