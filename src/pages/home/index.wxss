swiper {
  height: 400rpx;
  width: 100%;
}

swiper-item {
  height: 100%;
  width: 100%;
}

.slide-image {
  height: 100%;
  width: 100%;
}

.swiper-tab {
  width: 100%;
  text-align: center;
  line-height: 80rpx;
}

.swiper-tab-list {
  display: inline-block;
  width: 33.33%;
  color:rgba(51,51,51,1);
}
.swiper-tab-list text {
  font-size: 36rpx;
  display: inline-block;
  width: 33.33%;
}
.on {
  color: #FF3333;
  border-bottom: 4rpx solid #FF3333;
}

.swiper-box {
  display: block;
  height: 100%;
  width: 100%;
  /* overflow: hidden; */
  background: #fff;
}

.swiper-box view {
  text-align: center;
}

.seckill {
  width: 750rpx;
  height: 100%;
  margin-bottom: 28rpx;
  position: relative;
}

.seckill-header {
  width: 100%;
  height: 326rpx;
  background: #f33;
}

.header-title {
  display: flex;
  justify-content: space-between;
  height: 88rpx;
  line-height: 44rpx;
  margin: 0 20rpx;
}

.header-t {
  display: flex;
  overflow: hidden;
}

.header-tu {
  width: 70rpx;
  height: 70rpx;
  background-color: #fff;
  margin-top: 14rpx;
}

.header-text {
  line-height: 88rpx;
  font-size: 32rpx;
  color: #fff;
  margin-left: 11rpx;
}

.header-fl {
  display: flex;
}

.header-sao {
  display: flex;
  flex-direction: column;
  height: 88rpx;
  width: 100rpx;
  padding-top: 14rpx;
}

