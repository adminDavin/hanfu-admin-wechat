.sao-tu {
  width: 36rpx;
  height: 36rpx;
  margin: 0 auto;
}

.sao {
  line-height: 50rpx;
  color: #fff;
  font-size: 24rpx;
  text-align: center;
}

.sao-sou {
  width: 33rpx;
  height: 33rpx;
  margin: 21rpx 0 19rpx 20rpx;
  /* background: #000; */
}

.sou {
  display: flex;
  width: 710rpx;
  height: 72rpx;
  margin-left: 20rpx;
  margin-top: 27rpx;
  background: #fff;
  border-radius: 36rpx;
}

.sou-tu {
  width: 33rpx;
  height: 32rpx;
  margin: 21rpx 0 21rpx 20rpx;
  background-size: 33rpx;
  background: #000;
}

.input-sou {
  /* text-indent: 10rpx; */
  margin-left: 15rpx;
  width: 660rpx;
  height: 72rpx;
  line-height: 72rpx;
}

.seckill-img {
  width: 710rpx;
  height: 350rpx;
  position: absolute;
  top: 200rpx;
  left: 18rpx;
  background: #fff;
  margin: 0 auto;
  overflow: hidden;
  border-radius: 0.5rem;
}

.img-size {
  width: 710rpx;
  height: 100%;
  border-radius: 0.4rem;
}

swiper {
  width: 100%;
  height: 350rpx;
}

.seckill-box {
  width: 750rpx;
  height: 225rpx;
  background: #f8f8f8;
}

