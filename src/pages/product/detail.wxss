  height: 98rpx;
  background: red;
  position: fixed;
  bottom: 0;
  left: 0;
  background-color: #fff;
  overflow: hidden;
  display: flex;
  /* border-radius: 10px 10px 0px 0px; */
  justify-content: center;
}

.footer-inner {
  width: 90%;
  display: flex;
  justify-content: space-between;
  align-items: center;
}

.footter-service {
  display: flex;
  flex-direction: column;
  align-items: center;
}

.footer-image {
  width: 48rpx;
  height: 48rpx;
}

.foot-text {
  font-size: 24rpx;
  font-family: PingFang SC;
  font-weight: 400;
  color: rgba(21, 21, 21, 1);
  margin-top: 1rpx;
}

.footer-shppingcar {
  width: 170rpx;
  height: 68rpx;
  background: rgba(255, 51, 51, 1);
  /* border-radius: 34px; */
  line-height: 68rpx;
  text-align: center;
  font-size: 30rpx;
  font-family: 'PingFang SC';
  font-weight: 500;
  color: rgba(255, 255, 255, 1);
}

/* 选取地址 */
.delivery {
  font-size:28rpx;
  width: 100rpx;
  height: 21rpx;
  line-height: 21rpx;
  font-weight:bold;
  margin: 0 auto;
  margin-top: 20rpx;
  margin-bottom: 20rpx;
}
.newsites {
  width:360rpx;
  height: 70rpx;
  font-size:30rpx;
  font-weight: 500;
  text-align: center;
  line-height: 70rpx;
  margin: 0 auto;
  border-radius: 30rpx;
  color: #fff;
  background: rgba(255,51,51,1);
  margin-bottom: 20rpx;
}
.headbox {
  display: flex;
  margin: 16rpx 0;
}
.headPo {
  width:60rpx;
  height:60rpx;
  background: rgba(255,51,51,1);
  border-radius:50%;
  margin: 10rpx;
}
