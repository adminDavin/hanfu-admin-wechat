}

/* 商品区域 */

.shpping-content {
  width: 710rpx;
  height: 100%;
  margin: 0 auto;
  margin-top: 34rpx;
  display: flex;
  flex: 50%;
  display: -webkit-flex;
  justify-content: space-between;
  flex-direction: row;
  flex-wrap: wrap;
}

.content {
  width: 345rpx;
  height: 570rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
  /* float: left; */
  margin-bottom: 20rpx;
}

.content-images {
  width: 316rpx;
  height: 316rpx;
  margin-top: 13rpx;
}

.content-text {
  width: 316rpx;
  height: 56rpx;
  line-height: 56rpx;
  font-size: 26rpx;
  font-family: 'PingFang SC';
  font-weight: 500;
  color: rgba(21, 21, 21, 1);
  margin-top: 11rpx;
  /* overflow: hidden;
    text-overflow: ellipsis; *//* white-space: nowrap; */
}

.content-discounts {
  display: flex;
  align-items: center;
  flex-direction: column;
}

.discounts-money {
  width: 316rpx;
  height: 26rpx;
  color: #f00;
  margin-top: 22rpx;
  line-height: 26rpx;
  display: flex;
  align-items: center;
}

.money-title {
  font-size: 36rpx;
  font-weight: bold;
  font-family: 'PingFang-SC-Bold';
}

.money-number {
  font-family: 'PingFang-SC-Medium';
  font-size: 34rpx;
  font-weight: 500;
  margin-left: 3rpx;
}

.money-vip {
  width: 46rpx;
  height: 24rpx;
  margin-left: 19rpx;
}

.news {
  width: 316rpx;
  font-size: 24rpx;
  color: #f00;
  overflow: hidden;
}

.news-shpping {
  line-height: 35rpx;
  border: 1rpx solid #f00;
  border-radius: 10rpx;
  float: left;
  padding: 0 10rpx;
  margin-right: 20rpx;
  margin-top: 10rpx;
}

.news-green {
  color: #3c0;
  border: 1rpx solid #3c0;
}

.observer {
  width: 316rpx;
  float: left;
  line-height: 22rpx;
  font-size: 22rpx;
  color: #999;
  margin-top: 10rpx;
  margin-bottom: 19rpx;
}

.observe-one {
  width: 130rpx;
  height: 22rpx;
}

.top {
  width: 85rpx;
  height: 85rpx;
  margin-right: 10rpx;
  position: fixed;
  bottom: 74rpx;
  right: 0;
  z-index: 999;
}

.top-image {
  width: 85rpx;
  height: 85rpx;
  border-radius: 50%;
  background: rgba(0, 0, 0, 0.1);
  box-shadow: 0rpx 0 6rpx #a8a8a8;
}

.images-d {
  width: 33rpx;
  height: 36rpx;
  margin: 23rpx 27rpx 26rpx 25rpx;
  z-index: 99;
}

/* 筛选 */

.isRuleShow {
  display: block;
}

.isRuleHide {
  display: none;
}

.float {
  height: 100%;
  width: 750rpx;
  position: fixed;
  background-color: rgba(0, 0, 0, 0.5);
  z-index: 100;
  top: 0;
  left: 0;
}

.animation-element {
  width: 100%;
  height: 100%;
  background-color: #fff;
  border: 1px solid #f3f0f0;
  position: absolute;
  right: -130rpx;
  border-radius: 10px 0px 0px 10px;
}

.buttom {
  width: 579rpx;
  height: 70rpx;
  display: flex;
  justify-content: space-between;
  position: fixed;
  right: 19rpx;
  bottom: 37rpx;
}

.animation-reset {
  width: 263rpx;
  height: 70rpx;
  background: rgba(255, 255, 255, 1);
  border: 2rpx solid rgba(235, 235, 235, 1);
  border-radius: 35rpx;
  font-size: 32rpx;
  font-family: PingFang SC;
  font-weight: 500;
  line-height: 70rpx;
  text-align: center;
  color: rgba(21, 21, 21, 1);
}

.animation-button {
  background: rgba(255, 51, 51, 1);
  color: rgba(248, 248, 248, 1);
}

