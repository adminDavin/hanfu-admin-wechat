}

.colors {
  background: rgba(255, 51, 51, 1);
}

.sales {
  width: 58rpx;
  height: 26rpx;
  font-weight: 400;
  font-size: 28rpx;
  color: rgba(51, 51, 51, 1);
}

.table-flex {
  flex-direction: column;
}

.flex-box {
  display: flex;
  align-items: center;
}

.flex-box-two {
  width: 13rpx;
  height: 20rpx;
  display: flex;
  flex-direction: column;
}

.flex-imgae {
